<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>Servlet.Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/Servlet.Servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DatabaseAccesser</servlet-name>
        <servlet-class>Servlet.DatabaseAccesser</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DatabaseAccesser</servlet-name>
        <url-pattern>/Servlet.DatabaseAccesser</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Sync</servlet-name>
        <servlet-class>Servlet.Sync</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Sync</servlet-name>
        <url-pattern>/Servlet.Sync</url-pattern>
    </servlet-mapping>
</web-app>